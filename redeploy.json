{
	"targetServerIP": "ipaddress", // example: 127.0.0.1
	"targetServerUser": "user",
	"targetServerPwd": "password",
	"targetServerDeployPath": "targetDeployPath", // example: /home/deploy/appPackageName
	"targetPackage": "appPackageName",	// example: application
	"targetPackageType": "ear",	// example: ear(jar, war...)
	"sourcePackages": [
		"sourcePackage1", "sourcePackage2"
	],
	"packages": [
		{
			"sourcePackage1": {
				"packageType": "jar",
				"innerPackage": true,
				"filePath": "localPackageFilePath"	// example: local dev target files, if is maven project, the path usually is the target folder.
			},
			"sourcePackage2": {
				"packageType": "war",
				"innerPackage": false,
				"filePath": "localPackageFilePath"	// example: local dev target files, if is maven project, the path usually is the target folder.
			},
		}
	]
}